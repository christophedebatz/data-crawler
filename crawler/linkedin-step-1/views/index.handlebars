<h3>LinkedIn Step 1</h3>

<form method="post">
  <fieldset style="padding: 20px; border: 1px dotted lightGray;">
    <h4>Select a file to update...</h4>

    <select size="4" name="inputFileName" id="inputFileName" style="padding: 8px; margin: 10px; border: 1px solid lightGray;">
      {{#each files}}
        <option value="{{this}}">{{this}}</option>
      {{/each}}
    </select>

    <h4>Save as...</h4>
    <input type="text" id="outputFileName" placeholder="Type a file name..." size="35">
    <input type="submit" id="launch" value="Launch">
  </fieldset>
</form>

